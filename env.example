# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Stripe
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Stripe Product IDs (测试环境)
STRIPE_PRICE_MONTHLY=price_xxx
STRIPE_PRICE_YEARLY=price_yyy
STRIPE_PRICE_LIFETIME=price_zzz

# AI Models Configuration

# Anthropic (Claude) - 用于风格提取
ANTHROPIC_API_KEY=your_anthropic_api_key

# DeepSeek - 用于文案生成
DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_BASE_URL=https://api.deepseek.com

# 风格提取模型配置 (可选,默认使用 Claude)
EXTRACT_AI_PROVIDER=claude
EXTRACT_AI_MODEL=claude-sonnet-4-20250514

# 文案生成模型配置 (可选,默认使用 DeepSeek)
GENERATE_AI_PROVIDER=deepseek
GENERATE_AI_MODEL=deepseek-chat

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Subscription Check (开发环境可设置为 true 跳过订阅检查)
# 生产环境务必设置为 false 或不设置此变量
SKIP_SUBSCRIPTION_CHECK=true

